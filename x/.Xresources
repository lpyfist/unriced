*.font:go mono:size=11:style=regular
emacs.font: go mono:size=17:style=regular

urxvt.font: xft:go mono:size=11.2:style=regular, xft:symbola:size=11 

urxvt.boldFont: xft:go mono:size=11.3:style=bold
urxvt.italic: xft:go mono:size=11.3:style=italic

/* colors */
#include "/home/lpy/.themes/seashell"

Xft.dpi: 110
Xft.autohint: 1
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

uxxvt.letterSpace: 0
urxvt.lineSpace: 0
urxvt.geometry: 70x25
urxvt.internalBorder: 40
urxvt.intensityStyles: False
urxvt.scrollBar: False
urxvt.cursorBlink: True
urxvt.cursorUnderline: True
URxvt.perl-ext-common: default,clipboard

URxvt.clipboard.autocopy: true
URxvt.keysym.M-v: eval:paste_clipboard
URxvt.keysym.M-c: eval:selection_to_clipboard

*foreground:   base05
*background:   base00
*cursorColor:  base05

dmenu.selbackground: base00
dmenu.selforeground: base0D
dmenu.foreground: base03

*color0: base00
*color1: base08
*color2: base0B
*color3: base0A
*color4: base0D
*color5: base0E
*color6: base0C
*color7: base05

*color8:  base03
*color9:  base09
*color10: base01
*color11: base02
*color12: base04
*color13: base06
*color14: base0F
*color15: base07
